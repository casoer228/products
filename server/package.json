{
   "name": "new-typeorm-project",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "dependencies": {
      "@nestjs/common": "6.11.5",
      "@nestjs/core": "6.11.5",
      "@nestjs/platform-express": "7.0.9",
      "@nestjs/schedule": "^0.4.2",
      "@nestjs/swagger": "4.0.9",
      "@nestjs/typeorm": "6.2.0",
      "@nestjsx/crud": "4.4.1",
      "@nestjsx/crud-typeorm": "4.4.1",
      "@types/express": "4.17.6",
      "@types/lodash": "4.14.150",
      "class-transformer": "0.2.3",
      "class-validator": "0.10.0",
      "dotenv": "8.2.0",
      "express": "4.17.1",
      "nest-raven": "5.0.0",
      "node-fetch": "^2.6.1",
      "pg": "7.12.1",
      "reflect-metadata": "0.1.13",
      "rxjs": "6.5.4",
      "swagger-ui-dist": "3.30.0",
      "swagger-ui-express": "4.1.4",
      "typeorm": "0.2.19",
      "typeorm-naming-strategies": "1.1.0",
      "utf8": "^3.0.0",
      "winston": "^3.3.0",
      "winston-mongodb": "^5.0.1",
      "winston-sentry": "^0.2.1"
   },
   "scripts": {
      "prebuild": "rimraf dist",
      "pack": "npm run build && cp -a dist/ app",
      "pack:prod": "npm run pack && docker-compose build",
      "publish": "npm run pack:prod && docker-compose push",
      "build": "nest build && npm run postbuild",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "start": "ts-node -r tsconfig-paths/register src/main.ts",
      "watch": "ts-node-dev -r tsconfig-paths/register src/main.ts --watch",
      "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "start:typeorm": "npx nodemon -w ./src -e ts node_modules/.bin/ts-node src/main.ts",
      "db:sync": "cd src && npx ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js schema:sync -f=ormconfig.ts",
      "db:drop": "cd src && npx ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js schema:drop -f=ormconfig.ts",
      "db:seeds": "cd src && npx ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js migration:run -f=ormconfig.ts",
      "db:prepare": "npm run db:drop && npm run db:sync && npm run db:seeds",
      "ri": "rm -rf node_modules/@avidi package-lock.json && npm i",
      "postbuild": ""
   },
   "devDependencies": {
      "@nestjs/cli": "^7.5.1",
      "@nestjs/schematics": "^7.1.3",
      "@nestjs/testing": "^7.5.1",
      "@types/cron": "^1.7.2",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.8",
      "@types/jest": "^26.0.15",
      "@types/lodash": "^4.14.168",
      "@types/node": "^14.14.22",
      "@types/supertest": "^2.0.10",
      "@typescript-eslint/eslint-plugin": "^4.6.1",
      "@typescript-eslint/parser": "^4.6.1",
      "eslint": "^7.12.1",
      "eslint-config-prettier": "7.0.0",
      "eslint-plugin-prettier": "^3.1.4",
      "jest": "^26.6.3",
      "prettier": "^2.1.2",
      "supertest": "^6.0.0",
      "ts-jest": "^26.4.3",
      "ts-loader": "^8.0.8",
      "ts-node": "^9.0.0",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^4.0.5"
   }
}
